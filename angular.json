{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "ng-test": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng-test",
      "sourceRoot": "./apps/ng-test/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-test",
            "index": "./apps/ng-test/src/index.html",
            "main": "./apps/ng-test/src/main.ts",
            "polyfills": "./apps/ng-test/src/polyfills.ts",
            "tsConfig": "./apps/ng-test/tsconfig.app.json",
            "aot": false,
            "assets": ["./apps/ng-test/src/favicon.ico", "./apps/ng-test/src/assets"],
            "styles": ["./apps/ng-test/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "./apps/ng-test/src/environments/environment.ts",
                  "with": "./apps/ng-test/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-test:build",
            "port": 4300
          },
          "configurations": {
            "production": {
              "browserTarget": "ng-test:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "./apps/ng-test/src/polyfills.ts",
            "tsConfig": "./apps/ng-test/tsconfig.spec.json",
            "karmaConfig": "./apps/ng-test/karma.conf.js",
            "assets": ["./apps/ng-test/src/favicon.ico", "./apps/ng-test/src/assets"],
            "styles": ["./apps/ng-test/src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["tsconfig.app.json", "tsconfig.spec.json", "e2e/tsconfig.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "ng-test-e2e": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/ng-test-e2e",
      "sourceRoot": "apps/ng-test-e2e/src",
      "prefix": "app",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-test:serve:production"
            }
          }
        }
      }
    },
    "test-lib": {
      "projectType": "library",
      "root": "libs/test-lib",
      "sourceRoot": "libs/test-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "libs/test-lib/tsconfig.lib.json",
            "project": "libs/test-lib/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "libs/test-lib/src/test.ts",
            "tsConfig": "libs/test-lib/tsconfig.spec.json",
            "karmaConfig": "libs/test-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/test-lib/tsconfig.lib.json", "libs/test-lib/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "ng-test",
  "cli": {
    "defaultCollection": "@ngrx/schematics"
  }
}
